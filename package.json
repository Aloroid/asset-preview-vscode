{
  "name": "roblox-asset-preview-vscode",
  "displayName": "Roblox Asset Preview",
  "description": "Allows for the hovering of `rbxassetid://` to preview assets",
  "repository": {
    "type": "git",
    "url": "github.com/aloroid/asset-preview-vscode"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:lua",
    "onLanguage:luau"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Roblox Asset Preview",
      "properties": {
        "roblox-asset-preview-vscode.requirePrefix": {
          "title": "Require Prefix",
          "type": "boolean",
          "default": false,
          "description": "Requires `rbxassetid://` at the start of a number in order to show a preview"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}